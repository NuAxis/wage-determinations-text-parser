{
  "name": "wage-determinations-text-parser",
  "version": "1.0.0",
  "description": "JavaScript library for parsing wage determination text file. It takes wage determination file or content of text file found at https://www.wdol.gov/dba.aspx and produces JSON object described Wage Determination Object JSON Schema section.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint lib/**/*.js spec/**/*.js",
    "test": "npm run lint && npm run coverage && npm run check-coverage",
    "coverage": "istanbul cover _mocha -R spec/*.js",
    "check-coverage": "istanbul check-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:NuAxis/wage-determinations-text-parser.git"
  },
  "keywords": [
    "Wage Determination Parser",
    "WD Text Parser",
    "Wage Determinations Parser",
    "WageDeterminations Parser"
  ],
  "author": "Sarfraz Ahmad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NuAxis/wage-determinations-text-parser/issues"
  },
  "homepage": "https://github.com/NuAxis/wage-determinations-text-parser#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-json-schema": "^1.4.0",
    "eslint": "3.12.2",
    "eslint-config-google": "0.7.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.0.0"
  }
}
